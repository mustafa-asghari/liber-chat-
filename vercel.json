{
  "installCommand": "cd .. && npm ci --ignore-scripts",
  "buildCommand": "node build-vercel.cjs",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://librechatapi-production-1913.up.railway.app/api/:path*"
    },
    {
      "source": "/oauth/:path*",
      "destination": "https://librechatapi-production-1913.up.railway.app/oauth/:path*"
    }
  ]
}

